{
  "name": "prod-project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "webpack serve --env port=3000",
    "start:json:server": "ts-node ./json-server/index.ts",
    "build:prod": "webpack --env mode=production",
    "build:dev": "webpack --env mode=development",
    "lint:ts": "eslint . --ext .ts,.tsx",
    "lint:ts:fix": "eslint . --ext .ts,.tsx --fix",
    "lint:scss": "stylelint \"**/*.scss\"",
    "lint:scss:fix": "stylelint \"**/*.scss\" --fix",
    "format:check": "prettier --check \"**/*.{ts,tsx,scss}\"",
    "format:write": "prettier --write \"**/*.{ts,tsx,scss}\"",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:ui": "loki test",
    "test:ui:ok": "loki approve",
    "test:ui:ci": "loki --requireReference --reactUri file:./storybook-static",
    "test:ui:report": "ts-node ./scripts/generate-visual-json-report.ts && reg-cli --from .loki/report.json --report .loki/report.html",
    "analyze:prod": "webpack --env mode=production analyze",
    "analyze:dev": "webpack --env mode=development analyze",
    "storybook": "start-storybook -p 6006 -c ./config/storybook",
    "storybook:build": "build-storybook -c ./config/storybook"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^1.3.4",
    "i18next": "^22.4.9",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^12.1.5",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@svgr/webpack": "^6.5.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "12",
    "@types/jest": "^29.4.0",
    "@types/json-server": "^0.14.4",
    "@types/node": "^18.11.18",
    "@types/react": "^17.0.53",
    "@types/react-dom": "^17.0.18",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-i18next": "6.0.0-2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.10",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.4.2",
    "jest-environment-jsdom": "^29.4.2",
    "json-server": "^0.17.2",
    "loki": "^0.31.0",
    "mini-css-extract-plugin": "^2.7.2",
    "prettier": "^2.8.4",
    "reg-cli": "^0.17.7",
    "sass": "^1.58.0",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "stylelint": "^15.1.0",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  }
}
